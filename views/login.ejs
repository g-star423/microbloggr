<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs') %>
</head>

<body>
    <header>
        <%- include('./partials/header.ejs') %>
    </header>
    <h1 class="title">Login Page</h1>
    <div class="input-container">
        <form action="/login" method="POST">
            <div class="field">
                <label class="label" for="username">Username</label>
                <input class="input" name="username" type="text" name="username" /><br />
            </div>
            <div class="field">
                <label class="label" for="password">Password</label>
                <input class="input" name="password" type="password" name="password" /><br />
            </div>
    </div>
    <div class="button-container">
        <input type="submit" name="" value="Submit Login" class="button-54">
    </div>
    </form>
</body>

</html>